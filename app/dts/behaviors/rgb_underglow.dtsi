/ {
    behaviors {
        // Behavior can be invoked on peripherals, so name must be <= 8 characters.
        rgb_ug: rgb_ug {
            compatible = "zmk,behavior-rgb-underglow";
            #binding-cells = <2>;
            display-name = "Underglow";
        };
    };
};
